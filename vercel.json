{
    "routes": [
      {
        "src": "/images/(.*)",
        "dest": "/images/$1",
        "headers": {
          "Cache-Control": "public, max-age=31536000"
        }
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "status": 200
      }
    ]
  }